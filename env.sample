# Core API Keys
GROQ_API_KEY=your_groq_api_key_here
QDRANT_URL=your_qdrant_url_here
QDRANT_API_KEY=your_qdrant_api_key_here

# Model Settings
FAST_LLM=groq:mixtral-8x7b-32768
SMART_LLM=groq:mixtral-8x7b-32768
STRATEGIC_LLM=groq:mixtral-8x7b-32768
EMBEDDING=text-embedding-3-small

# LiveKit Configuration
# Get these credentials from your LiveKit Cloud account or self-hosted LiveKit server
LIVEKIT_API_KEY=your_livekit_api_key_here
LIVEKIT_API_SECRET=your_livekit_api_secret_here
LIVEKIT_WS_URL=wss://your-livekit-server.livekit.cloud

# Speech-to-Text Configuration (Faster Whisper)
# Options: tiny, base, small, medium, large-v1, large-v2, large-v3
# Smaller models are faster but less accurate
WHISPER_MODEL=base
# Set to 1 to use GPU acceleration if available
USE_CUDA=0

# Text-to-Speech Configuration
TTS_MODEL_PATH=Kokoro-82M

# Audio processing settings
AUDIO_SAMPLE_RATE=16000
AUDIO_CHUNK_SIZE=4096
